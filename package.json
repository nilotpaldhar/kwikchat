  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "dev:email": "email dev --dir src/email-templates",
    "clean": "rimraf .next",
    "lint:check": "next lint",
    "lint:fix": "eslint . --ext ts --ext tsx --ext js --fix",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "check:all": "npm run lint:check && npm run format:check  && npm run build",
    "fix:all": "npm run lint:fix && npm run format:fix",
    "prisma:migrate": "dotenv -e .env prisma migrate dev",
    "prisma:push": "dotenv -e .env prisma db push",
    "prisma:studio": "dotenv -e .env prisma studio",
    "db:seed": "tsx src/lib/seeder/seed.ts",
    "prepare": "husky",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nilotpaldhar/storekeeper"
  },
